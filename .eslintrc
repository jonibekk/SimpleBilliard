{
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "window": true,
    "cake": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-uses-vars": 1,
    no-console: ["error", { allow: ["warn", "error"] }],
    "no-alert": 1,
    "no-unused-vars": 1,
    "prefer-const": 1,
    "object-shorthand": 1,
    "indent": [2, 2, {"SwitchCase": 1}],
    "comma-dangle": 1,
  }
}
